{
	"$id": "http://localhost:8080/json-schema/getOneManagedAttributeJSONSchema.json",
	"base": "http://localhost:8080/api",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "The managed attributes schema",
	"type": "object",
	"description": "The managed attributes for the media meta data",
	"required": [
		"data"
	],
	"properties": {
		"data": {
			"required": [
				"id",
				"type",
				"attributes"
			],
			"properties": {
				"id": {
					"type": "string",
					"description": "ref to the identifier of the attribute",
					"examples": [
						"1",
						"2",
						"3",
						"4"
					]
				},
				"type": {
					"const": "managedAttribute",
					"description": "The type of the object being returned."
				},
				"attributes": {
					"required": [
						"name"
					],
					"properties": {
						"name": {
							"type": "string",
							"description": "ref to the name of the attribute",
							"examples": [
								"specimen_view",
								"identifier",
								"identifier_scope",
								"type_status"
							]
						},
						"accepted values": {
							"type": "array",
							"items": [
								{
									"type": "string",
									"description": "accepted value of the given managed attribute"
								}
							],
							"description": "ref to the accepted values of the named attribute",
							"examples": [
								"dosal for specimen_view",
								"CataloguedObject for identifier_scope"
							]
						}
					}
				}
			}
		}
	}
}
