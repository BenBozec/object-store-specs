{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://localhost:8080/json-schema/getOneFilenameMetadataStrategySchema.json",
	"$base": "https://localhost:8080/api/",
	"title": "The strategy creation schema.",
	"type": "object",
	"required": [
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"data": {
			"type": "object",
			"required": [
				"id",
				"type",
				"attributes"
			],
			"properties": {
				"id": {
					"type": "string",
					"description": "Identifier of the strategy."
				},
				"type": {
					"const": "strategy",
					"description": "The strategy used to parse a file name."
				},
				"attributes": {
					"strategy": {
						"type": "object",
						"attributes": {
							"strategyName": {
								"type": "string",
								"description": "Name of the strategy."
							},
							"regex": {
								"type": "string",
								"description": "Pattern used to parse file name."
							},
							"targets": {
								"type": "array",
								"items": {
									"type": "object",
									"description": "Attributes captured from file name."
								}
							}
						}
					}
				}
			}
		}
	}
}
